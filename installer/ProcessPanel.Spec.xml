<processing>
	<job name="Setting Up Repository">
		<executeclass name="org.callimachusproject.installer.RepositorySetupProcess" />
	</job>
	<job name="Initializing Server">
		<executeclass name="org.callimachusproject.installer.StartServerProcess" />
	</job>
	<job name="Starting Server">
		<executeclass name="org.callimachusproject.installer.PrimeServerProcess" />
	</job>
	<onFail previous="false" next="false" />
	<onSuccess previous="false" next="true" />
</processing>
