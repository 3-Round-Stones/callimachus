<!-- META-INF/xmlcalabash.xml -->
<xproc-config xmlns="http://xmlcalabash.com/ns/configuration"
	      xmlns:calli="http://callimachusproject.org/rdf/2009/framework#">

<implementation type="calli:render"
		class-name="org.callimachusproject.xproc.RenderStep" />

<implementation type="calli:render-sparql-query"
		class-name="org.callimachusproject.xproc.RenderSparqlQueryStep" />

<implementation type="calli:sparql"
		class-name="org.callimachusproject.xproc.SparqlStep" />

<implementation type="calli:decode-text"
		class-name="org.callimachusproject.xproc.DecodeTextStep" />

<implementation type="calli:serialize-json"
		class-name="org.callimachusproject.xproc.SerializeJsonStep" />

<implementation type="calli:deserialize-json"
		class-name="org.callimachusproject.xproc.DeserializeJsonStep" />

</xproc-config>
