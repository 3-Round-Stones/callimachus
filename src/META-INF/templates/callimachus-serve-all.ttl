@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix skos:<http://www.w3.org/2004/02/skos/core#>.
@prefix sd:<http://www.w3.org/ns/sparql-service-description#>.
@prefix void:<http://rdfs.org/ns/void#>.
@prefix foaf:<http://xmlns.com/foaf/0.1/>.
@prefix msg:<http://www.openrdf.org/rdf/2011/messaging#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.
@prefix :<#>.

################################
# Other Resources are Public
################################

<> a </callimachus/SchemaGraph>.

<LocalResource> a owl:Class;
    rdfs:label "Local Resource";
    msg:matching </*>.

# All foreign resources (diverted requests) are served publicly
<ForeignResource> a owl:Class;
    rdfs:label "Foreign Resource";
    owl:complementOf <LocalResource>;
    rdfs:subClassOf </callimachus/Serviceable>;
    calli:reader </group/public>;
    calli:subscriber </group/everyone>.

