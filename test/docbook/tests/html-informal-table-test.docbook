<article 
    xmlns      ="http://docbook.org/ns/docbook" 
    xmlns:xl   ="http://www.w3.org/1999/xlink"
    version="5.0">
    <title>An HTML table without a title</title>
    <informaltable>
        <col/>
        <col/>
        <thead>
            <tr>
                <td>Head 1</td>
                <td>Head 2</td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>Body 1</td>
                <td>Body 2</td>
            </tr>
        </tbody>
    </informaltable>
</article>
