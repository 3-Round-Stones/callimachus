<launch4jConfig>
  <headerType>gui</headerType>
  <outfile>Callimachus-Setup.exe</outfile>
  <dontWrapJar>false</dontWrapJar>
  <errTitle>Callimachus Setup</errTitle>
  <supportUrl>http://callimachusproject.org/</supportUrl>
  <priority>normal</priority>
  <customProcName>true</customProcName>
  <stayAlive>true</stayAlive>
  <icon>images/callimachus.ico</icon>
  <singleInstance>
    <mutexName>Callimachus Setup</mutexName>
    <windowTitle>Callimachus Setup</windowTitle>
  </singleInstance> 
  <jre>
    <minVersion>1.6.0_18</minVersion>
    <maxVersion>1.6.9_99</maxVersion>
    <jdkPreference>preferJdk</jdkPreference>
    <!-- Heap sizes in MB and % of free memory -->
    <initialHeapSize>64</initialHeapSize>
    <maxHeapSize>768</maxHeapSize>
    <opt>-Dswing.aatext=true</opt>
    <opt>-Dfile.encoding=UTF-8</opt>
    <opt>-DuseJavaUtilZip</opt>
  </jre>
  <versionInfo>
    <fileDescription>Callimachus Setup Tool</fileDescription>
    <copyright>Copyright 2012 3 Round Stones Inc.</copyright>
    <productName>Callimachus Setup</productName>
    <companyName>3 Round Stones Inc.</companyName>
    <internalName>Callimachus-Setup</internalName>
    <originalFilename>Callimachus-Setup.exe</originalFilename>
  </versionInfo>
  <messages>
    <startupErr>Callimachus Setup could not be started in this environment.</startupErr>
    <bundledJreErr>This application was configured to use a bundled Java Environment but it is missing or corrupted.</bundledJreErr>
    <jreVersionErr>Before you can run this tool you must install Java JDK</jreVersionErr>
    <launcherErr>The registry refers to a nonexistent Java installation or it is corrupted. Please unistall all Java software and install Java again before running this tool</launcherErr>
  </messages>
</launch4jConfig>
