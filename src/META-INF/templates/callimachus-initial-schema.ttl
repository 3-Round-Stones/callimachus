@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix skos:<http://www.w3.org/2004/02/skos/core#>.
@prefix msg:<http://www.openrdf.org/rdf/2011/messaging#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.
@prefix audit:<http://www.openrdf.org/rdf/2009/auditing#>.
@prefix :<#>.

################################################################
# Schema data to initialize an Callimachus store, but may be removed.
################################################################

audit:current-trx a calli:SchemaGraph.

################################
# Protected Resource Classes
################################

</callimachus/Change>             msg:realm </callimachus/manifest>.
</callimachus/Group>              msg:realm </callimachus/manifest>.
</callimachus/Photo>              msg:realm </callimachus/manifest>.
</callimachus/SparqlEndPoint>     msg:realm </callimachus/manifest>.
</callimachus/User>               msg:realm </callimachus/manifest>.

################################
# Who can create what
################################

</callimachus/User>            calli:author </group/admin>.
</callimachus/Group>           calli:author </group/admin>.
</callimachus/File>            calli:author </group/admin>.
</callimachus/NamedGraph>      calli:author </group/admin>.
</callimachus/NamedQuery>      calli:author </group/admin>.

</callimachus/Folder>          calli:author </group/staff>,</group/admin>.
</callimachus/Class>           calli:author </group/staff>,</group/admin>.
</callimachus/Concept>         calli:author </group/staff>,</group/admin>.
</callimachus/Article>         calli:author </group/staff>,</group/admin>.
</callimachus/Page>            calli:author </group/staff>,</group/admin>.
</callimachus/Script>          calli:author </group/staff>,</group/admin>.
</callimachus/Style>           calli:author </group/staff>,</group/admin>.
</callimachus/Text>            calli:author </group/staff>,</group/admin>.
</callimachus/Image>           calli:author </group/staff>,</group/admin>.
</callimachus/AnimatedGraphic> calli:author </group/staff>,</group/admin>.
</callimachus/IconGraphic>     calli:author </group/staff>,</group/admin>.
</callimachus/NetworkGraphic>  calli:author </group/staff>,</group/admin>.
</callimachus/Photo>           calli:author </group/staff>,</group/admin>.
</callimachus/VectorGraphic>   calli:author </group/staff>,</group/admin>.

