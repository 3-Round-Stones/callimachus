@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix skos:<http://www.w3.org/2004/02/skos/core#>.
@prefix foaf:<http://xmlns.com/foaf/0.1/>.
@prefix msg:<http://www.openrdf.org/rdf/2011/messaging#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.
@prefix :<#>.

<> a <RdfSchemaGraph>;
    foaf:primaryTopic <Concept>.

<Concept> a <Creatable>, owl:Class;
    rdfs:subClassOf <Viewable>, <Editable>, skos:Concept;
    rdfs:label "Concept";
    rdfs:comment "Concepts are ideas, meanings, or categories of objects and events.";
    owl:equivalentClass </callimachus/Concept>, </callimachus/1.0/types/Concept>;
    rdfs:isDefinedBy <../../ontology>;
    calli:administrator </auth/groups/super>;
    calli:author </auth/groups/users>,</auth/groups/staff>,</auth/groups/power>,</auth/groups/admin>;
    calli:icon <../images/concept.png>;
    calli:thumbnail <../images/concept.svg>;
    calli:view <../templates/concept-view.xhtml>;
    calli:edit <../templates/concept-edit.xhtml>;
    calli:create <../templates/concept-create.xhtml>.

