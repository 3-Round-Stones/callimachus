@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix obj:<http://www.openrdf.org/rdf/2009/object#>.
@prefix http:<http://www.openrdf.org/rdf/2009/httpobject#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.

@base <http://callimachusproject.org/rdf/2009/framework>.

################################
# Ontology
################################

<> a owl:Ontology.

################################
# Class
################################

calli:Alias a owl:Class;
	rdfs:comment "Moved Permantently (301)".

calli:Copyable a owl:Class;
	rdfs:comment "Resources that can be copied to create new resources.".

calli:Deletable a owl:Class;
	rdfs:comment "Resources that can be removed from the RDF store.".

calli:Editable a owl:Class;
	rdfs:comment "Resources that can be edited.".

calli:Viewable a owl:Class;
	rdfs:comment "Resources that can be viewed from a Web browser.".

calli:Reviewable a owl:Class;
	rdfs:comment "Resources that can be viewed from a Web browser.";
	rdfs:subClassOf calli:Viewable.

calli:Realm a owl:Class;
	rdfs:comment "A realm.".

calli:AnybodyRealm a owl:Class;
	rdfs:subClassOf calli:Realm;
	rdfs:comment "A realm that authenticates any agent.".

calli:NobodyRealm a owl:Class;
	rdfs:subClassOf calli:Realm;
	rdfs:comment "A realm that forbids all agents.".

calli:LocalRealm a owl:Class;
	rdfs:subClassOf calli:Realm;
	rdfs:comment "A realm that only authenticates local agents.".

calli:DigestRealm a owl:Class;
	rdfs:subClassOf calli:Realm;
	rdfs:comment "A realm that authenticates HTTP digest credentials.".

calli:Credential a owl:Class;
	rdfs:comment "A digest credentials with a MD5 algorithm.";
	rdfs:subClassOf calli:Party.

calli:Group a owl:Class;
	rdfs:comment "A collection of credentials.";
	rdfs:subClassOf calli:Party.

calli:Party a owl:Class;
	rdfs:comment "A credential or group of credentials.".

calli:NamedGraph a owl:Class;
	rdfs:comment "Graph in the RDF store that can be retrieved remotely.".

calli:SPARQLService a owl:Class;
	rdfs:comment "A resource that can respond to SPARQL queries.".

################################
# Annotation
################################

calli:copy a owl:AnnotationProperty;
	rdfs:comment "The RDFa template used to copy this resource using a Web browser.".

calli:delete a owl:AnnotationProperty;
	rdfs:comment "The RDFa template used to remove this resource using a Web browser.".

calli:edit a owl:AnnotationProperty;
	rdfs:comment "The RDFa template used to edit this resource using a Web browser.".

calli:view a owl:AnnotationProperty;
	rdfs:comment "The RDFa template used to display this resource in a Web browser.".

calli:review a owl:AnnotationProperty;
	rdfs:comment "The RDFa template used to display this resource's history in a Web browser.".

################################
# Property
################################

calli:algorithm a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:comment "A value of 'MD5' indicates this credential is a digest credential.";
	rdfs:domain calli:Credential;
	rdfs:range xsd:string.

calli:encoded a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:comment "The MD5 encoded username:realm:password";
	rdfs:domain calli:Credential;
	rdfs:range xsd:hexBinary.

calli:name a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:comment "The username for this credential.";
	rdfs:domain calli:Credential;
	rdfs:range xsd:string.

calli:credential a owl:ObjectProperty;
	rdfs:comment "Credential or group of credentials that can be authenticated by this realm.";
	rdfs:domain calli:DigestRealm;
	rdfs:range calli:Party.

calli:authName a rdf:Property, owl:FunctionalProperty;
	rdfs:comment "A string to be displayed to users so they know which username and password to use. This string should contain at least the name of the host performing the authentication and might additionally indicate the collection of users who might have access. This string is used to encode the passwords and changing it will invalidates existing encoded passwords.";
	rdfs:domain calli:DigestRealm.

calli:origin a rdf:Property;
	rdfs:comment "Identifies the security contexts that cause user agent scripts to initiate an HTTP request that can be seamlessly authenticated with this realm or '*' for all contexts.";
	rdfs:domain calli:Realm.

calli:domain a rdf:Property;
	rdfs:comment "Define the protection space. Any URI that has a URI in this set as a prefix (after both have been made absolute) may be assumed to be in the same protection space.";
	rdfs:domain calli:Realm.

calli:scribe a owl:ObjectProperty;
	rdfs:comment "Credential or group of credentials that can copy this resource remotely.";
	rdfs:domain calli:Copyable;
	rdfs:range calli:Party.

calli:curator a owl:ObjectProperty;
	rdfs:comment "Credential or group of credentials that will maintain any copied resources.";
	rdfs:domain calli:Copyable;
	rdfs:range calli:Party.

calli:reviewer a owl:ObjectProperty;
	rdfs:comment "Credential or group of credentials that can view this resource remotely.";
	rdfs:domain calli:Viewable;
	rdfs:range calli:Party.

calli:maintainer a owl:ObjectProperty;
	rdfs:comment "Credential or group of credentials that can interact with this resource remotely.";
	rdfs:range calli:Party.

calli:member a owl:ObjectProperty;
	rdfs:comment "Credential that is part of this group.";
	rdfs:domain calli:Group;
	rdfs:range calli:Credential.

calli:soundex a owl:DatatypeProperty;
	rdfs:comment "A soundex encoding of one of this resource's label or label prefix.";
	rdfs:range xsd:string.

calli:aliasOf a owl:ObjectProperty, owl:FunctionalProperty;
	http:method "GET";
	http:type "text/uri-list";
	http:expect "301-moved-permantently";
	rdfs:comment "The canonical IRI for this alias.";
	rdfs:domain calli:Alias.

################################
# Individual
################################

calli:nobody a calli:NobodyRealm.

