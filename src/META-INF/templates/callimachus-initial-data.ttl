@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix skos:<http://www.w3.org/2004/02/skos/core#>.
@prefix msg:<http://www.openrdf.org/rdf/2011/messaging#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.
@prefix audit:<http://www.openrdf.org/rdf/2009/auditing#>.
@prefix :<#>.

################################################################
# Data to initialize an Callimachus store, but may be removed.
################################################################

################################
# Backwards compatibility
################################

</layout/template.xsl> a calli:Alias; calli:aliasOf </callimachus/manifest?template>.

################################
# Authorization and Groups
################################

</callimachus/accounts> a </callimachus/AccountManager>, calli:AccountManager;
	rdfs:label "User accounts";
	calli:authName </>;
	calli:domain </>;
	calli:authNamespace </user/>.

</> a </callimachus/Folder>, calli:Folder, calli:Alias, calli:Origin;
	rdfs:label "Home folder";
	rdfs:comment "Create new folders here to organize resources and assign permissions";
	calli:version "0.13";
	calli:aliasOf </page/main-page.xhtml?view>;
	calli:reader </group/users>;
	calli:editor </group/staff>;
	calli:administrator </group/admin>;
	calli:isNamespaceOf </group/>, </user/>, </sparql>.

</user/> a </callimachus/Folder>, calli:Folder;
	rdfs:label "user";
	rdfs:comment "Create new users here and be sure to add them to groups";
	calli:reader </group/staff>;
	calli:administrator </group/admin>.

</group/> a </callimachus/Folder>, calli:Folder;
	rdfs:label "group";
	rdfs:comment "Create new groups here";
	calli:reader </group/staff>;
	calli:administrator </group/admin>;
	calli:isNamespaceOf </group/admin>, </group/staff>, </group/users>.

</group/admin> a calli:Party, calli:Group, </callimachus/Group>;
	rdfs:label "admin";
	rdfs:comment "The user accounts in this group have heightened privileges, including the ability to edit other user accounts and access the underlying data store";
	calli:reader </group/staff>;
	calli:administrator </group/admin>.

</group/staff> a calli:Party, calli:Group, </callimachus/Group>;
	rdfs:label "staff";
	rdfs:comment "Members of this group can design and edit resources within the system";
	calli:reader </group/staff>;
	calli:administrator </group/admin>.

</group/users> a calli:Party, calli:Group, </callimachus/Group>;
	rdfs:label "users";
	rdfs:comment "Members of this group can view and discuss resources within the system";
	calli:reader </group/staff>;
	calli:administrator </group/admin>.

################################
# Well known services
################################

</sparql> a </callimachus/SparqlEndPoint>;
	rdfs:label "sparql";
	calli:administrator </group/admin>.

</callimachus/profile> a </callimachus/Profile>.

################################
# Manifest and Menu
################################

</callimachus/manifest> a </callimachus/Manifest>, calli:Manifest;
	rdfs:label "manifest";
	calli:reader </group/users>;
	calli:editor </group/staff>;
	calli:administrator </group/admin>;
	calli:origin </>;
	calli:template </callimachus/styles/layout.xsl>;
	calli:unauthorized </callimachus/pages/unauthorized.xhtml>;
	calli:forbidden </callimachus/pages/forbidden.xhtml>;
	calli:authentication </callimachus/accounts>;
	calli:logo </callimachus/images/callimachus-logo.png> ;
	calli:favicon </callimachus/images/callimachus-icon.ico> ;
	calli:theme </callimachus/themes/default>;
	calli:colourScheme </callimachus/themes/default>.

</callimachus/menu> a </callimachus/Menu>, calli:Menu;
	rdfs:label "menu";
	calli:reader </group/users>;
	calli:editor </group/staff>;
	calli:administrator </group/admin>;
	calli:link </>;
	calli:item </callimachus/menu#site>;
	calli:item </callimachus/menu#resources>;
	calli:item </callimachus/menu#administration>;
	calli:item </callimachus/menu#toolbox>.

</callimachus/menu#site> calli:position 1; rdfs:label "Site";
	calli:item </callimachus/menu#mainarticle>;
	calli:item </callimachus/menu#homefolder>;
	calli:item </callimachus/menu#contents>;
	calli:item </callimachus/menu#recentchanges>.
</callimachus/menu#resources> calli:position 8; rdfs:label "Resources";
	calli:item </callimachus/menu#folders>;
	calli:item </callimachus/menu#articles>;
	calli:item </callimachus/menu#pages>;
	calli:item </callimachus/menu#concepts>;
	calli:item </callimachus/menu#classes>.
</callimachus/menu#administration> calli:position 9; rdfs:label "Administration";
	calli:item </callimachus/menu#manifest>;
	calli:item </callimachus/menu#users>;
	calli:item </callimachus/menu#groups>;
	calli:item </callimachus/menu#namedqueries>;
	calli:item </callimachus/menu#sparqlquery>.
</callimachus/menu#toolbox> calli:position 10; rdfs:label "Toolbox";
	calli:item </callimachus/menu#whatlinkshere>;
	calli:item </callimachus/menu#relatedchanges>;
	calli:item </callimachus/menu#introspectresource>;
	calli:item </callimachus/menu#permissions>;
	calli:item </callimachus/menu#printthispage>.

</callimachus/menu#mainarticle> calli:position 1; rdfs:label "Main article"; calli:link </page/main-page.xhtml?view>.
</callimachus/menu#homefolder> calli:position 2; rdfs:label "Home folder"; calli:link </?view>.
</callimachus/menu#contents> calli:position 3; rdfs:label "Contents"; calli:link </callimachus/menu>.
</callimachus/menu#recentchanges> calli:position 4; rdfs:label "Recent changes"; calli:link </callimachus/changes>.

</callimachus/menu#folders> calli:position 1; rdfs:label "Folders"; calli:link </callimachus/Folder>.
</callimachus/menu#articles> calli:position 2; rdfs:label "Articles"; calli:link </callimachus/Article>.
</callimachus/menu#pages> calli:position 3; rdfs:label "Pages"; calli:link </callimachus/Page>.
</callimachus/menu#concepts> calli:position 4; rdfs:label "Concepts"; calli:link </callimachus/Concept>.
</callimachus/menu#classes> calli:position 5; rdfs:label "Classes"; calli:link </callimachus/Class>.

</callimachus/menu#manifest> calli:position 1; rdfs:label "Manifest"; calli:link </callimachus/manifest>.
</callimachus/menu#users> calli:position 2; rdfs:label "Users"; calli:link </callimachus/User>.
</callimachus/menu#groups> calli:position 3; rdfs:label "Groups"; calli:link </callimachus/Group>.
</callimachus/menu#namedqueries> calli:position 4; rdfs:label "Named queries"; calli:link </callimachus/NamedQuery>.
</callimachus/menu#sparqlquery> calli:position 5; rdfs:label "SPARQL query"; calli:link </sparql>.

</callimachus/menu#whatlinkshere> calli:position 1; rdfs:label "What links here"; calli:link <javascript:location='?whatlinkshere'>.
</callimachus/menu#relatedchanges> calli:position 2; rdfs:label "Related changes"; calli:link <javascript:location='?relatedchanges'>.
</callimachus/menu#introspectresource> calli:position 3; rdfs:label "Introspect resource"; calli:link <javascript:location='?introspect'>.
</callimachus/menu#permissions> calli:position 3; rdfs:label "Permissions"; calli:link <javascript:location='?permissions'>.
</callimachus/menu#printthispage> calli:position 4; rdfs:label "Print this page"; calli:link <javascript:print()>.

