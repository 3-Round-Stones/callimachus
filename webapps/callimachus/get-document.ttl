:GetXsl rdfs:subClassOf obj:Message;
	rdfs:subClassOf [owl:onProperty obj:target;
		owl:allValuesFrom <urn:mimetype:text/xsl>];
	rdfs:subClassOf [owl:onProperty obj:functionalObjectResponse;
		owl:allValuesFrom <java:java.io.InputStream>];
	http:method "GET";
	http:type "text/xsl";
	http:cache-control "cache-range";
	obj:script "return openInputStream()".

