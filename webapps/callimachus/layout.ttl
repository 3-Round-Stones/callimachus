@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix skos:<http://www.w3.org/2004/02/skos/core#>.
@prefix obj:<http://www.openrdf.org/rdf/2009/object#>.
@prefix http:<http://www.openrdf.org/rdf/2009/httpobject#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.
@prefix audit:<http://www.openrdf.org/rdf/2009/auditing#>.
@prefix :<#>.

:TransformLayout rdfs:subClassOf obj:Message;
	rdfs:subClassOf [owl:onProperty obj:functionalObjectResponse; owl:allValuesFrom <java:java.io.InputStream>];
	http:type "application/xhtml+xml";
	http:transform :xhtml-to-html-xsl;
	obj:xslt <../layout/template.xsl>.

:input a owl:ObjectProperty; a owl:FunctionalProperty;
	http:type "application/xhtml+xml";
	rdfs:domain :TransformLayout;
	rdfs:range <java:java.io.InputStream>.

:query a owl:DatatypeProperty; a owl:FunctionalProperty;
	rdfs:domain :TransformLayout;
	rdfs:range xsd:string;
	http:parameter "*".

:xhtml-to-html-xsl rdfs:subClassOf obj:Message;
	rdfs:subClassOf [owl:onProperty obj:functionalObjectResponse; owl:allValuesFrom <java:java.io.Reader>];
	http:type "text/html";
	obj:xslt <util/xhtml-to-html.xsl>.

:xhtml a owl:ObjectProperty; a owl:FunctionalProperty;
	http:type "application/xhtml+xml";
	rdfs:domain :xhtml-to-html-xsl;
	rdfs:range <java:java.io.InputStream>.

