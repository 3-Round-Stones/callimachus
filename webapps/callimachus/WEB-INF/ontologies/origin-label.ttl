@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix msg:<http://www.openrdf.org/rdf/2011/messaging#>.
@prefix calli:<http://callimachusproject.org/rdf/2009/framework#>.
@prefix :<#>.

:InitializeOriginlabel owl:equivalentClass calli:Started;
	msg:imports <java:org.openrdf.model.vocabulary>;
	msg:script """
		var con = this.objectConnection;
		var vf = con.valueFactory;
		var calli = "http://callimachusproject.org/rdf/2009/framework#";
		var Origin = vf.createURI(calli, "Origin");

		var origins = con.getStatements(null, RDF.TYPE, Origin, true, []);
		try {
			while (origins.hasNext()) {
				var origin = origins.next().subject;
				if (!con.hasStatement(origin, RDFS.LABEL, null, true, [])) {
					var url = origin.toString();
					if (url.lastIndexOf('/') == url.length - 1) {
						var label = url.substring(0, url.length - 1);
						con.add(origin, RDFS.LABEL, vf.createLiteral(label), []);
					}
				}
			}
		} finally {
			origins.close();
		}
		proceed();
	""".

